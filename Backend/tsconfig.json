{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@app/*": [
        "src/*"
      ],
      "@components/*": [ 
        "src/components/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }
  }
}